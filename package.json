    "e2e": "ng e2e",
    "lint": "ng lint",
    "clean": "./scripts/clean.sh",
    "ci": "./scripts/ci.sh",
    "sync-version-fields": "./scripts/sync-version-fields.js",
    "version": "npm run sync-version-fields && git add projects/fullcalendar/package.json"
  },
  "dependencies": {
    "@angular/animations": "^14.2.4",
